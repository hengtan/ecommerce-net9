{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=CatalogDb;User Id=sa;Password=YourPassword;",
    "RedisConnection": "localhost:6379"
  },
  "ElasticSearch": {
    "Uri": "http://localhost:9200",
    "IndexFormat": "catalog-logs-{0:yyyy.MM.dd}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
